{
  "name": "photoprism-helper",
  "version": "1.0.0",
  "description": "An unofficial companion tool created for use alongside PhotoPrism to enable api endpoints and automation.",
  "main": "build/api.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "build:docker": "docker build -t aerilym/photoprism-helper .",
    "start:dev": "nodemon",
    "start:prd": "node build/api.js",
    "reinstall:wa": "npm remove winston-axios && npm install winston-axios"
  },
  "keywords": [
    "PhotoPrism",
    "API",
    "helper"
  ],
  "author": "Ryan Miller",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "node-cron": "^3.0.1",
    "puppeteer": "^16.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-axios": "^1.7.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.47",
    "@types/node-cron": "^3.0.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
